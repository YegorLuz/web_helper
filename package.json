  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "html-react-parser": "^0.9.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-loadable": "^5.5.0",
  }
